<Window x:Class="DerbyApp.Windows.SelectCamera"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DerbyApp.Windows"
        mc:Ignorable="d"
        Title="Select Camera" Width="350" Height="110">
    <Grid Width="323">
        <ComboBox Name="cbCamera" HorizontalAlignment="Left" Margin="10,10,10,0" VerticalAlignment="Top" Width="300" ItemsSource="{Binding Path=., Mode=TwoWay}" Height="22"/>
        <Button Content="OK" HorizontalAlignment="Left" Margin="43,37,0,10" VerticalAlignment="Top" Width="100" Height="20" Click="Button_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Right" Margin="0,37,43,10" VerticalAlignment="Top" Width="100" Height="20" Click="Button_Click_1"/>
    </Grid>
</Window>
