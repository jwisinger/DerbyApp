<Page x:Class="DerbyApp.Pages.Help"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DerbyApp.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Help">

    <Grid Margin="50,50,50,50">
        <Grid.Background>
            <ImageBrush Stretch="Uniform" ImageSource="/Images/GIRL.jpg" Opacity="0.1"/>
        </Grid.Background>
        <TextBlock Name="HelpBox" TextWrapping="Wrap" Margin="0,0,0,0"/>
    </Grid>
</Page>
